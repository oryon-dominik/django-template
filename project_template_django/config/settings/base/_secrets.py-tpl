import environ


def define_secrets(env: environ.Env) -> dict:
    """
    Define secrets for the project.
    
    will be read from .env files or the environment provided.
    """
    ADMIN_URL = env.str("DJANGO_ADMIN_URL", default="secretadmin-h7Y8lCVH9/")  # type: ignore[no-untyped-call]
    # POSTGRES_PASSWORD = env.str("POSTGRES_PASSWORD")
    # we will not return the env
    del env
    return locals()
